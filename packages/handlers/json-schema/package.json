{
  "name": "@graphql-mesh/json-schema",
  "version": "0.20.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "json-machete": "0.0.18",
    "@graphql-mesh/types": "0.52.0",
    "@graphql-mesh/utils": "0.18.0",
    "@graphql-mesh/store": "0.1.17",
    "@graphql-tools/utils": "8.3.0",
    "graphql-compose": "9.0.3",
    "@omnigraph/json-schema": "0.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
