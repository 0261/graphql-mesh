{
  "name": "@graphql-mesh/runtime",
  "version": "0.17.0",
  "license": "MIT",
  "peerDependencies": {
    "graphql": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "dependencies": {
    "@n1ru4l/graphql-live-query": "0.7.1",
    "@n1ru4l/in-memory-live-query-store": "0.6.5",
    "@graphql-mesh/types": "0.42.0",
    "@graphql-mesh/utils": "0.12.0",
    "@graphql-tools/delegate": "beta",
    "@graphql-tools/batch-delegate": "beta",
    "@graphql-tools/resolvers-composition": "beta",
    "@graphql-tools/schema": "beta",
    "@graphql-tools/wrap": "beta",
    "@graphql-tools/utils": "beta",
    "chalk": "4.1.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
